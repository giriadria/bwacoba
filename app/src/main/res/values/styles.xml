<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="generalnotitle" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowNoTitle">true</item>
    </style>
    
    <style name="ratingbar" parent="Widget.AppCompat.RatingBar">
        <item name="android:minHeight">18dp</item>
        <item name="android:maxHeight">18dp</item>
    </style>
    <style name="euy" parent="Theme.AppCompat.NoActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/colorPrimary</item>
    <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    <item name="colorAccent">@color/colorAccent</item>

    </style>
    <style name="uye" parent="Theme.AppCompat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
